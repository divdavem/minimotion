{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es2015", "dom"],
    "module": "ESNext",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "outDir": "temp",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "baseUrl": "./node_modules",
    "allowUnusedLabels": true
  }
}
